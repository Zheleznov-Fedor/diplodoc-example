    src-dark: https://raw.githubusercontent.com/Zheleznov-Fedor/diplodoc-example/main/logo-dark.svg
    src-preview:
      ru: https://raw.githubusercontent.com/Zheleznov-Fedor/diplodoc-example/main/share-doc-ru.png
      default: https://raw.githubusercontent.com/Zheleznov-Fedor/diplodoc-example/blob/main/share-doc-en.png
    title: Design 30ki
    img-has:
      offset: true
